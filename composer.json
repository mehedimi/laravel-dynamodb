{
    "name": "mehedimi/laravel-dynamodb",
    "version": "0.2.0",
    "description": "A package for Laravel to communicate DynamoDB",
    "type": "package",
    "license": "MIT",
    "keywords": ["laravel dynamodb", "dynamodb", "database"],
    "homepage": "https://github.com/mehedimi/laravel-dynamodb",
    "authors": [
        {
            "name": "Mehedi Hasan",
            "email": "mehedihasansabbirmi@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Mehedi\\LaravelDynamoDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mehedi\\LaravelDynamoDB\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6|^7|^8",
        "illuminate/pagination": "^6|^7|^8",
        "illuminate/database": "^6|^7|^8",
        "aws/aws-sdk-php": "^3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mehedi\\LaravelDynamoDB\\DynamoDBServiceProvider"
            ]
        }
    }
}
